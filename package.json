{
  "name": "jsonweaver",
  "version": "1.1.3",
  "type": "module",
  "description": "A simple utility to transform JSON data into CSV, XML, YAML, JSONLines and Markdown table formats.",
  "keywords": [
    "csv",
    "xml",
    "json",
    "data",
    "yaml",
    "format",
    "markdown",
    "json-lines",
    "jsonweaver",
    "data-transform"
  ],
  "author": {
    "name": "Kledenai",
    "email": "me@kledenai.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kledenai/jsonweaver.git"
  },
  "bugs": {
    "url": "https://github.com/kledenai/jsonweaver/issues"
  },
  "homepage": "https://github.com/kledenai/jsonweaver#readme",
  "license": "MIT",
  "main": "dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "node --experimental-vm-modules node_modules/.bin/jest",
    "lint": "eslint src",
    "start": "node dist/index.js",
    "prepare": "npm run build",
    "clean": "rm -rf dist"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {
    "ajv": "^8.17.1",
    "js-yaml": "^4.1.0",
    "xmlbuilder": "^15.1.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/js-yaml": "^4.0.9",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "tsup": "^8.4.0",
    "typescript": "^5.7.2"
  }
}
